{
    "name": "ariby/ulid",
    "description": "Laravel package Use to auto product primary key.",
    "type": "library",
    "authors": [
        {
            "name": "Ariby",
            "email": "defsrisars@gmail.com"
        },
        {
            "name": "Liao San Kai",
            "email": "liaosankai@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.2",
        "Illuminate/Support": "~5.3",
        "robinvdvleuten/ulid": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "vlucas/phpdotenv": "~2.0",
        "orchestra/testbench": "3.6.*",
        "orchestra/database": "3.6.*"
    },
    "autoload": {
        "psr-4": {
            "Ariby\\Ulid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ariby\\Ulid\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ariby\\Ulid\\UlidServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
